@article{Tate.2021,
  title = {Flood Exposure and Social Vulnerability in the {{United States}}},
  author = {Tate, Eric and Rahman, Md Asif and Emrich, Christopher T. and Sampson, Christopher C.},
  year = {2021},
  month = mar,
  journal = {Natural Hazards},
  volume = {106},
  number = {1},
