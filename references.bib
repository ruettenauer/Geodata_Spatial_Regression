@article{Debus.2016,
  title = {Parlamentswahl in Der {{Diktatur}}: {{Eine Analyse}} Des {{W\"ahlerverhaltens}} Auf {{Ebene}} Der {{Land-}} Und {{Stadtkreise}} Bei Der {{Reichstagswahl}} Vom 12. {{November}} 1933},
  author = {Debus, Marc},
  year = {2016},
