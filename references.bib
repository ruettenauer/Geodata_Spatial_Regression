@article{Shiffer-Sebba.2025,
  title = {Keeping the {{Family Fortune}}: {{How Bureaucratic Practices Preserve Elite Multigenerational Wealth}}},
  shorttitle = {Keeping the {{Family Fortune}}},
  author = {{Shiffer-Sebba}, Doron},
  year = {2025},
  month = feb,
  journal = {American Sociological Review},
  pages = {00031224251319001},
  issn = {0003-1224, 1939-8271},
  doi = {10.1177/00031224251319001},
  abstract = {How do wealthy families preserve their fortunes across generations? A historic peak in wealth inequality in the United States has inspired research on how economic elites benefit from markets, tax rates, and legal entities. However, the ongoing practices through which families maintain their fortunes across generations are less understood. Using six months of ethnographic observations at a wealth manager for the top 0.1 percent, as well as interviews with the manager's clients and a wider sample of managers, I argue that wealthy families adopt what I call ``bureaucratic practices''\textemdash activities like meetings, presentations, and signing documents\textemdash to preserve wealth intergenerationally. After erecting legal entities such as corporations, trusts, and foundations, wealth managers help wealthy families implement bureaucratic practices. These practices, which privilege bureaucratic form over substance, constitute a crucial behavioral layer atop the legal infrastructure, facilitating a greater degree of wealth preservation compared with using entities alone. Thus, preserving wealth at the top should be understood not merely as a set of discrete transfers from parents to children, but as an enduring multigenerational process of professional socialization that introduces new behaviors into family life.},
  langid = {english}
}

