@article{Fingleton.2020,
  title = {Estimating the Local Employment Impacts of Immigration: {{A}} Dynamic Spatial Panel Model},
  shorttitle = {Estimating the Local Employment Impacts of Immigration},
  author = {Fingleton, Bernard and Olner, Daniel and Pryce, Gwilym},
  year = {2020},
  month = oct,
  journal = {Urban Studies},
  volume = {57},
  number = {13},
  pages = {2646--2662},
  issn = {0042-0980, 1360-063X},
  doi = {10.1177/0042098019887916},
  abstract = {This paper highlights a number of important gaps in the UK evidence base on the employment impacts of immigration, namely: (1) the lack of research on the local impacts of immigration \textendash{} existing studies only estimate the impact for the country as a whole; (2) the absence of long-term estimates \textendash{} research has focused on relatively short time spans \textendash{} there are no estimates of the impact over several decades, for example; (3) the tendency to ignore spatial dependence of employment which can bias the results and distort inference \textendash{} there are no robust spatial econometric estimates we are aware of. We aim to address these shortcomings by creating a unique data set of linked Census geographies spanning five Censuses since 1971. These yield a large enough sample to estimate the local impacts of immigration using a novel spatial panel model which controls for endogenous selection effects arising from migrants being attracted to high-employment areas. We illustrate our approach with an application to London and find that no migrant group has a statistically significant long-term negative effect on employment. EU migrants, however, are found to have a significant positive impact, which may have important implications for the Brexit debate. Our approach opens up a new avenue of inquiry into subnational variations in the impacts of immigration on employment.},
  langid = {english},
  file = {N\:\\Zotero\\Fingleton et al_2020_Estimating the local employment impacts of immigration.pdf}
}

