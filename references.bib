@article{Inglehart.1995,
  title = {Public {{Support}} for {{Environmental Protection}}: {{Objective Problems}} and {{Subjective Values}} in 43 {{Societies}}},
  shorttitle = {Public {{Support}} for {{Environmental Protection}}},
  author = {Inglehart, Ronald},
  year = {1995},
