@article{Charles.2003,
  title = {The {{Dynamics}} of {{Racial Residential Segregation}}},
  author = {Charles, Camille Zubrinsky},
  year = {2003},
  month = aug,
  journal = {Annual Review of Sociology},
  volume = {29},
