  shorttitle = {Disaster Risk, Climate Change, and Poverty},
  author = {Winsemius, Hessel C. and Jongman, Brenden and Veldkamp, Ted I.E. and Hallegatte, Stephane and Bangalore, Mook and Ward, Philip J.},
