@article{Catney.2023,
  title = {Ethnic Diversification and Neighbourhood Mixing: {{A}} Rapid Response Analysis of the 2021 {{Census}} of {{England}} and {{Wales}}},
  shorttitle = {Ethnic Diversification and Neighbourhood Mixing},
  author = {Catney, Gemma and Lloyd, Christopher D. and Ellis, Mark and Wright, Richard and Finney, Nissa and Jivraj, Stephen and Manley, David},
