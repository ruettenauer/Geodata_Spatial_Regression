@article{Battig.2009,
  title = {National {{Institutions}} and {{Global Public Goods}}: {{Are Democracies More Cooperative}} in {{Climate Change Policy}}?},
  shorttitle = {National {{Institutions}} and {{Global Public Goods}}},
  author = {B{\"a}ttig, Mich{\`e}le B. and Bernauer, Thomas},
  year = {2009},
