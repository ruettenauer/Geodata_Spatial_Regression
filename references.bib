@article{Mitze.2020,
  title = {Face Masks Considerably Reduce {{COVID-19}} Cases in {{Germany}}},
  author = {Mitze, Timo and Kosfeld, Reinhold and Rode, Johannes and W{\"a}lde, Klaus},
