@article{Raftery.2012,
  title = {Bayesian Probabilistic Population Projections for All Countries},
  author = {Raftery, Adrian E. and Li, Nan and {\v S}ev{\v c}{\'i}kov{\'a}, Hana and Gerland, Patrick and Heilig, Gerhard K.},
  year = {2012},
