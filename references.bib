@article{Oliu-Barton.2022,
  title = {The Effect of {{COVID}} Certificates on Vaccine Uptake, Health Outcomes, and the Economy},
  author = {{Oliu-Barton}, Miquel and Pradelski, Bary S. R. and Woloszko, Nicolas and {Guetta-Jeanrenaud}, Lionel and Aghion, Philippe and Artus, Patrick and Fontanet, Arnaud and Martin, Philippe and Wolff, Guntram B.},
