@article{Viel.2011,
  title = {Environmental Justice in a {{French}} Industrial Region: Are Polluting Industrial Facilities Equally Distributed?},
  author = {Viel, Jean-Fran{\c c}ois and H{\"a}gi, Mathieu and Upegui, Erika and Laurian, Lucie},
  year = {2011},
