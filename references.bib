@article{Jackson.2007,
  title = {How {{Far Merit Selection}}? {{Social Stratification}} and the {{Labour Market}}},
  author = {Jackson, Michelle},
  year = {2007},
  journal = {The British Journal of Sociology},
  volume = {58},
  number = {3},
