@article{Biegert.2023,
  title = {They {{Can}}'t {{All Be Stars}}: {{The Matthew Effect}}, {{Cumulative Status Bias}}, and {{Status Persistence}} in {{NBA All-Star Elections}}},
  shorttitle = {They {{Can}}'t {{All Be Stars}}},
  author = {Biegert, Thomas and K{\"u}hhirt, Michael and Van Lancker, Wim},
